{
  "name": "inyourhead",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "build": "vite build && npm run copy-geoip",
    "copy-geoip": "mkdir -p dist/server/data && cp -r server/data/*.mmdb dist/server/data/ 2>/dev/null || true",
    "vercel-build": "vite build && npm run copy-geoip",
    "lint": "eslint .",
    "preview": "vite preview",
    "client": "vite",
    "server": "node ./server/server.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "@maxmind/geoip2-node": "^6.3.4",
    "@turf/turf": "^7.3.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "d3": "^7.9.0",
    "express": "^5.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.0",
    "topojson-client": "^3.1.0",
    "vite": "^7.2.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0"
  }
}